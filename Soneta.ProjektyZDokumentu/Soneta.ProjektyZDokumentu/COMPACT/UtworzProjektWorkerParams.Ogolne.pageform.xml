<?xml version="1.0"?>

<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.enova.pl/schema/form.xsd"
          RightName="Page:Tworzenie"
          Priority="100000"
          DataType="Soneta.UtworzProjektWorkerParams,Soneta">
  <Page Name="Tworzenie" CaptionHtml="Tworzenie projektu" Width="100" Height="14" DataContext="{DataSource}">
    <Group>
      <Field CaptionHtml="Prowadzący" Width="*"  EditValue="{Prowadzacy}" />
      <Field CaptionHtml="Czy chcesz edytować nazwę projektu?" Width="*"  EditValue="{CzyZmienicNazwe}" />
      <Field CaptionHtml="Nazwa projektu" Width="*" Visibility="{CzyZmienicNazwe}" EditValue="{NazwaProjektu}" />
      <Field CaptionHtml="Data rozpoczęcia" Width="*"  EditValue="{DataRozpoczecia}" />
      <Field CaptionHtml="Czy przenieść planowany przychód" Width="*"  EditValue="{CzyPrzeniescPrzychod}" />
    </Group>
    <Group>
      <Label CaptionHtml="COMPACT" Class="BoldLabel" Width="*"></Label>
      <Label CaptionHtml="Zabrze, Hagera 41" Class="BoldLabel" Width="*"></Label>
    </Group>
  </Page>
</DataForm>

